<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>Admin dashboard</title>
    </head>
    <body>
        <h2>Access granted to the admin.</h2>
        <% for(let i=0; i<candidates.length; i++) { %>
            <hr>
            <p>Name: <%= candidates[i].name %></p>
            <p>E-mail: <%= candidates[i].email %></p>
            <p>Roll number: <%= candidates[i].roll_no %></p>
            <p>Rank: <%= candidates[i].rank %></p>
            <!-- <p>Preferences: <%= candidates[i].preferences %></p> -->
            <% for(let j=0; j<candidates[i].preferences.length; j++) { %>
                <% if(j === 0) continue; %>
                <p style="color: blue;"><%= j %>. <%= candidates[i].preferences[j] %></p>
            <% } %>
        <% } %>
    </body>
</html>